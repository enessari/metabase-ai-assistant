{
  "name": "metabase-ai-assistant",
  "version": "4.2.0",
  "mcpName": "io.github.enessari/metabase-ai-assistant",
  "description": "The most powerful MCP Server for Metabase - 134 tools with structured output, AI-powered SQL generation, dashboard automation, user management & enterprise BI. MCP SDK v1.26.0 compliant. Works with Claude, Cursor, and any MCP-compatible AI.",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "mcp": "node src/mcp/server.js",
    "mcp:server": "node src/mcp/server.js",
    "mcp:prod": "NODE_ENV=production node src/mcp/server.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop metabase-ai-mcp",
    "pm2:restart": "pm2 restart metabase-ai-mcp",
    "pm2:logs": "pm2 logs metabase-ai-mcp",
    "docker:build": "docker build -t metabase-ai-mcp .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f metabase-ai-mcp",
    "test": "jest",
    "test:connection": "node src/mcp/server.js --test-connection",
    "lint": "eslint src/**/*.js"
  },
  "bin": {
    "metabase-ai-mcp": "./src/mcp/server.js"
  },
  "keywords": [
    "mcp",
    "mcp-server",
    "model-context-protocol",
    "metabase",
    "metabase-api",
    "metabase-mcp",
    "ai",
    "llm",
    "claude",
    "cursor",
    "openai",
    "anthropic",
    "database",
    "postgresql",
    "mysql",
    "dashboard",
    "analytics",
    "sql",
    "sql-generator",
    "natural-language-sql",
    "text-to-sql",
    "business-intelligence",
    "bi",
    "data-visualization",
    "query-builder",
    "ai-assistant",
    "automation",
    "enterprise",
    "data-analytics"
  ],
  "author": "Abdullah Enes SARI <contact@onmartech.com> (ONMARTECH LLC)",
  "license": "Apache-2.0",
  "files": [
    "src/",
    "README.md",
    "README_MCP.md",
    "LICENSE",
    ".env.example"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/enessari/metabase-ai-assistant.git"
  },
  "homepage": "https://github.com/enessari/metabase-ai-assistant#readme",
  "bugs": {
    "url": "https://github.com/enessari/metabase-ai-assistant/issues"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.17.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "axios": "^1.6.5",
    "chalk": "^5.3.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "he": "^1.2.0",
    "inquirer": "^12.9.3",
    "jsonwebtoken": "^9.0.3",
    "mysql2": "^3.14.3",
    "openai": "^4.26.0",
    "ora": "^8.0.1",
    "pg": "^8.16.3",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "eslint": "^8.56.0",
    "jest": "^29.7.0"
  }
}